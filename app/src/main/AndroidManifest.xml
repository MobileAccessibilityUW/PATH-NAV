<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.boruili.path_nav" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NavActivity" >
        </activity>
        <activity android:name=".TrainActivity" >
        </activity>
        <activity android:name=".TrainPathActivity" >
        </activity>
        <activity android:name=".NavPathActivity" >
        </activity>
        <activity
            android:name=".ListTrainActivity"
            android:label="@string/title_activity_list_train"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".ListNavActivity" >
        </activity>
    </application>

</manifest>
